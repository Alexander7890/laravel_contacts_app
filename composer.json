{
    "name": "kostin/laravel-contacts-app",
    "description": "Simple contacts manager for university lab (Laravel 11).",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}